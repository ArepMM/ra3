<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>

        <!-- Уставка давления в цилиндрах отжатия стояночного тормоза (ЦСТ), МПа -->
        <pPB_max>0.4</pPB_max>
        <!-- Уставка давления в тормозных цилиндрах (ТЦ), МПа -->
        <pBC_max>0.38</pBC_max>
        <!-- Условное отношение площадей управляющего поршня (коэффициент к давлению в ТЦ) -->
        <A>0.47</A>
        <!-- Эквивалентное давление от пружин управляющего поршня (уравновешивающее ТМ), МПа -->
        <ps>0.49</ps>
        <!-- Номинальное напряжение питания -->
        <U_nom>110.0</U_nom>

        <!-- Коэффициент наполнения ЦСТ из ГР (отпуск стояночного тормоза) -->
        <K1>1.5e-3</K1>
        <!-- Коэффициент разрядки ЦСТ в атмосферу (применение стояночного тормоза) -->
        <K2>1.0e-3</K2>

        <!-- Коэффициенты к условному положению управляющего поршня -->
        <K3>10.0</K3>
        <K5>10.0</K5>

        <!-- Коэффициент наполнения ТЦ из редуктора от управляющего поршня -->
        <K4>2.0e-3</K4>
        <!-- Коэффициент разрядки ТЦ в атмосферу от управляющего поршня -->
        <K6>2.0e-3</K6>

        <!-- Коэффициент наполнения ТЦ из редуктора от тормозного вентиля ЭПТ -->
        <K7>2.0e-3</K7>
        <!-- Коэффициент разрядки ТЦ в атмосферу от отпускного вентиля ЭПТ -->
        <K8>2.0e-3</K8>

        <!-- Точность поддержания давления в ТЦ при ЭПТ, МПа -->
        <ept_eps>0.01</ept_eps>

        <!-- Минимальная ступень торможения ЭПТ -->
        <!-- <p_min>0.1</p_min> -->

    </Device>
</Config>
