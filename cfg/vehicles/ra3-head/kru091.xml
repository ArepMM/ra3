<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Device>
		<Order>1</Order>
		<SubStepNum>1</SubStepNum>
		<Solver>euler</Solver>

		<!-- Объём рабчего (уравнительного) резервуара -->
		<EqReservoirVolume>2.0e-3</EqReservoirVolume>

		<!-- Коэффициент утечек из РР -->
		<EqResLeak>1.0e-6</EqResLeak>

		<PosDelay>300</PosDelay>

		<!-- Коэффициент зарядки РР из редуктора в положении отпуска -->
		<K1>4.0e-4</K1>
		<!-- Коэффициент разрядки РР в атмосферу в положении торможения -->
		<K2>1.0e-4</K2>

		<!-- Коэффициент к разнице давлений ТМ и РР (условный уравнительный поршень) -->
		<A>0.8</A>

		<!-- Коэффициент зарядки ТМ из ГР от уравнительного поршня -->
		<K3>2.0e-2</K3>
		<!-- Коэффициент разрядки ТМ в атмосферу от уравнительного поршня -->
		<K4>1.5e-2</K4>

	</Device>
</Config>
