<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <Order>2</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Число тормозных цилиндров в тормозной рычажной передаче -->
        <CylindersNum>4</CylindersNum>

        <!-- Диаметр тормозного цилиндра, м -->
        <CylindersDiameter>0.19</CylindersDiameter>

        <!-- Максимальный выход штока, м -->
        <StockOut>0.132</StockOut>

        <!-- Мёртвый объём тормозных цилиндров и подводящих труб, м^3 -->
        <DeadVolume>1.0e-3</DeadVolume>

        <!-- Коэффициент утечек из тормозных цилиндров -->
        <kLeak>1.0e-6</kLeak>

        <!-- Давление в ТЦ - начало движения поршня из отпускного положения -->
        <pBegin>0.02</pBegin>

        <!-- Давление в ТЦ - окончание движения поршня в тормозное положение -->
        <pEnd>0.04</pEnd>

        <!-- Максимальное давление в ТЦ -->
        <pMax>0.4</pMax>

        <!-- Максимальное нажатие на колодку (при максимальном давлении), тс -->
        <Kmax>1.45</Kmax>

        <!-- Число колодок на каждой оси -->
        <!-- <ShoesAxis>2</ShoesAxis> -->

        <!-- Эффективный радиус тормозной поверхности (радиус поверхности катания колёс или тормозных дисков) -->
        <!-- <EffRadius>0.430</EffRadius> -->

        <!-- Материал колодок: iron/iron-ph/composite (чугун/фосфористый чугун/композит) -->
        <ShoeType>composite</ShoeType>

        <!-- Начальное давление в тормозных цилиндрах -->
        <InitPressure>0.0</InitPressure>

        <!-- Число цилиндров отжатия пружинных энергоаккумуляторов стояночного тормоза -->
        <num_ea>2</num_ea>

        <!-- Объём цилиндров отжатия пружинных энергоаккумуляторов стояночного тормоза -->
        <V_st>0.005</V_st>

    </Device>
</Config>